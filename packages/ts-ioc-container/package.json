{
  "name": "ts-ioc-container",
  "version": "1.3.8",
  "description": "Typescript IoC container",
  "author": "ibabkin <ibabkin@goodgamestudios.com>",
  "homepage": "https://github.com/IgorBabkin/service-locator/tree/master/packages/ts-ioc-container",
  "license": "ISC",
  "sideEffects": false,
  "main": "cjm/index.js",
  "types": "esm/index.d.ts",
  "module": "esm/index.js",
  "keywords": [
    "typescript",
    "ioc",
    "service-locator",
    "di"
  ],
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "cjm/**/*",
    "esm/**/*",
    "docs/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IgorBabkin/service-locator"
  },
  "bugs": {
    "url": "https://github.com/IgorBabkin/service-locator/issues"
  },
  "scripts": {

    "build": "npm-run-all -p build:**",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "typescript": "4.0.5"
  },

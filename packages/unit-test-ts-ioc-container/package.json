{
  "name": "unit-test-ts-ioc-container",
  "version": "1.4.0",
  "description": "Unit test decorator for Typescript IoC container",
  "author": "ibabkin <ibabkin@goodgamestudios.com>",
  "homepage": "https://github.com/IgorBabkin/service-locator/tree/master/packages/unit-test-ts-ioc-container",
  "license": "ISC",
  "sideEffects": false,
  "main": "cjm/index.js",
  "types": "esm/index.d.ts",
  "module": "esm/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "cjm/**/*",
    "esm/**/*"
  ],
  "keywords": [
    "typescript",
    "ioc",
    "service-locator",
    "di",
    "unit",
    "test",
    "mock"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IgorBabkin/service-locator"
  },
  "bugs": {
    "url": "https://github.com/IgorBabkin/service-locator/issues"
  },
  "scripts": {
    "build:cjs": "rimraf cjm && tsc -p tsconfig.json --outDir cjm --module CommonJS --target ES5",
    "build:esm": "rimraf esm && tsc -p tsconfig.json --outDir esm",
    "build": "npm-run-all -p build:**",
    "prepublish": "yarn build",
    "test": "jest"
  },
  "dependencies": {
    "moq.ts": "^6.4.0",
    "ts-ioc-container": "^1.5.0"
  },
  "devDependencies": {
    "@types/jest": "26.0.15",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "ts-jest": "26.4.3",
    "typescript": "4.0.5"
  },
  "gitHead": "e81ee0e14def051fa47d1a12b1c9f01cf1e512e8"
}
